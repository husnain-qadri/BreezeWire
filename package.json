{
  "name": "breezewire-frontend",
  "version": "1.0.0",
  "description": "Modern React TypeScript frontend for BreezeWire - A comprehensive weather application with interactive maps and real-time forecasts",
  "type": "module",
  "author": "BreezeWire Team",
  "license": "MIT",
  "keywords": [
    "weather",
    "react",
    "typescript",
    "vite",
    "bootstrap",
    "google-maps",
    "highcharts",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/husnain-qadri/BreezeWire.git"
  },
  "bugs": {
    "url": "https://github.com/husnain-qadri/BreezeWire/issues"
  },
  "homepage": "https://github.com/husnain-qadri/BreezeWire#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "serve": "vite preview --host --port 3000"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.3",
    "@types/google.maps": "^3.58.1",
    "@types/react-bootstrap": "^0.32.37",
    "@vis.gl/react-google-maps": "^1.4.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "highcharts": "^11.4.8",
    "highcharts-react-official": "^3.2.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}